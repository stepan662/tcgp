grammar = (
  {
    # non-terminals
    FUNC, TYPE, PARAMS, PARAMS_N, BODY
  },
  {
    # terminals
    'id', 'int', 'float', '(', ')', '{', '}', ',', ';'
  },
  {
    # rules
    FUNC    -> TYPE 'id' '(' PARAMS ')' '{' BODY '}';
    TYPE    -> 'int';
    TYPE    -> 'float';
    PARAMS  -> TYPE 'id' PARAMS_N;
    PARAMS  -> ;
    PARAMS_N-> ',' TYPE 'id' PARAMS_N;
    PARAMS_N-> ;
    BODY    -> ';';
  },
  # start symbol
  FUNC
)
